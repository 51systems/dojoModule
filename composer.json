  "type": "library",
  "keywords": [
    "dojo",
    "zf2"
  ],
  "homepage": "https://github.com/51systems/dojoModule",
  "authors": [
    {
      "name": "Tim Roediger",
      "email": "superdweebie@gmail.com"
    },
    {
      "name": "Dustin Thomson",
      "email": "dthomson@51systems.com"
    }
  ],
  "require": {
    "php": ">=5.3",
    "zendframework/zend-mvc": "^2 || ^3",
    "zendframework/zend-stdlib": ">=2.5.0",
    "zendframework/zend-view": ">=2.5.0",
    "zendframework/zend-modulemanager": ">=2.5.0",
    "zendframework/zend-config": ">=2.5.0",
    "zendframework/zend-json": ">=2.5.0",


    "doctrine/doctrine-orm-module": "^1"
  },
  "require-dev": {
    "phpunit/PHPUnit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Dojo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DojoTest\\": "test/"
    }
  }
